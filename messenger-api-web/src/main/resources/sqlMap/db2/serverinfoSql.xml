<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- PUSH receiver sqlMap_ORACLE -->
<mapper namespace="serverinfo">
    <select id="getServerInfo" parameterType="hashmap" resultType="kr.msp.push.receiver.model.ServerInfoBean">
        <![CDATA[
        SELECT * from T_PUSH_SERVER_INFO where ISACTIVE='Y' order by SERVERID ASC
        ]]>
    </select>

    <select id="getServerGroup" parameterType="hashmap" resultType="kr.msp.push.receiver.model.ServerGroupInfoBean">
        <![CDATA[
        SELECT * from T_PUSH_SERVER_GROUP order by GROUPID ASC
        ]]>
    </select>
</mapper>